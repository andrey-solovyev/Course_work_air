                                                                                        <complexType>
                                                                                            <attribute name="EndNumber" type="integer">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify an end of range line number in the PNR's ticketing field to view air extra-related information for</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="eTicketNumber" type="integer">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify an eTicket number to view air extra-related information for</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Number" type="integer">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify a particular line from the PNR's ticketing field to view air extra-related information for.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Ind" type="boolean" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify for the system to include air extras</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Summary" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>This attribute is not in use</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="BaggageAllowance" minOccurs="0" maxOccurs="unbounded">
                                                                            <annotation>
                                                                                <documentation>Used to define baggage allowance details</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Segment" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify the relevant itinerary segments</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Segment number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="EndNumber" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Final number in range</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Number" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the quantity of checked bags. @Number and @Weight cannot combine</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Weight" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the weight in kilos of checked bags.@Number and @Weight cannot combine</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Commission" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Details of the commission being claimed</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Amount" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the numeric amount of commission being claimed if applicable.Amount and Percent are mutually exclusive</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Percent" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the percentage of commission being claimed if applicable. Amount and Percent are mutually exclusive</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Customer" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify the DK customer number</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Number" type="string">
                                                                                    <annotation>
                                                                                        <documentation>DK number</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Endorsements" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify miscellaneous text in the endorsements field</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Text" type="string">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify miscellaneous text in the endorsements field</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Override" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Override any endorsement</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="HemisphereCode" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Hemisphere code</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="JourneyCode" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Journey code</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="MultiTicket" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to instruct the system to consider multi-ticket solutions to find the lowest possible price</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Ind" type="boolean" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to consider multi-ticket solutions to find the lowest possible price</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="TourCode" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Tour code details</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="SuppressFareReplaceWithBT" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify to suppress the fare amount on the ticket and replace with BT</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify to suppress the fare amount on the ticket and replace with BT</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="SuppressFareReplaceWithIT" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify to suppress the fare amount on the ticket and replace with IT</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify to suppress the fare amount on the ticket and replace with IT</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="SuppressIT" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify to to suppress the IT in the tourcode box from printing</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify to to suppress the IT in the tourcode box from printing</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="SuppressITSupressFare" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify to specify to suppress IT from printing in the tour box on the ticket and to suppress fare amounts from printing on the ticket</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify to specify to suppress IT from printing in the tour box on the ticket and to suppress fare amounts from printing on the ticket</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Text" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify tour code</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="ValidationMethod" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify a validation method</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                            <element name="PricingQualifiers" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>All the price specific variables</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="Account" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify an account code to price with</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Code" type="string" maxOccurs="4">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify an account code to price with</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Force" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to force the specific account code</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="AlternativePricing" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Please note that the AlternativePricing node can only be used together with .../PriceRequestInformation/@AlternativePricing set to "true"</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Overrides" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Overrides</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="MatchNoMatch" minOccurs="0">
                                                                                                    <annotation>
                                                                                                        <documentation>Used to instruct the system to return both match and no match alternative pricing options</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <attribute name="Ind" type="boolean">
                                                                                                            <annotation>
                                                                                                                <documentation>Used to instruct the system to return both match and no match alternative pricing options</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                                <element name="NoMatch" minOccurs="0">
                                                                                                    <complexType>
                                                                                                        <attribute name="Ind" type="boolean">
                                                                                                            <annotation>
                                                                                                                <documentation>Used to instruct the system to return no match alternative pricing options</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="NoAvailCheck" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to request alternative pricing regardless of availability</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="BankersSellingRate" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to instruct the system to price an itinerary according to a specified banker's selling rate.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="BargainFinder" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to instruct the system to search out lower fares based upon the existing itinerary segments</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Historical" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to return the lowest fare ever offered for the particular itinerary regardless of whether or not it is currently available</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Rebook" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to rebook the flights in the applicable classes of service to get the lower fare</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="BasicEconomyExclude" type="string" minOccurs="0" maxOccurs="5">
                                                                            <annotation>
                                                                                <documentation>Ued to exclude Basic Economy Fares (up to 5 booking classes can be selected). Please note that this element is only applicable to airline customers. "BasicEconomyExclude" can only be used with .../BargainFinder </documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="Brand" minOccurs="0" maxOccurs="unbounded">
                                                                            <annotation>
                                                                                <documentation>Used to specify a brand.  Please note that it's not possible to request more than 1 brand for an entire journey, or for the same segment</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <simpleContent>
                                                                                    <extension base="string">
                                                                                        <attribute name="RPH" type="integer">
                                                                                            <annotation>
                                                                                                <documentation>Used to match up with "SegmentSelect" Brand/@RPH cannot combine with .../CommandPricing/@RPH</documentation>
                                                                                            </annotation>
                                                                                        </attribute>
                                                                                    </extension>
                                                                                </simpleContent>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="BuyingDate" type="spct:date" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to instruct the system to specify the purchase date for the fare. "BuyingDate" follows this format: YYYY-MM-DD</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="Cabin" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to instruct the system to specify fares by a specific cabin or the lowest fare across all cabins. Please note that Cabin may only be used together with .../PriceRequestInformation/OptionalQualifiers/PricingQualifiers/BargainFinder or .../PriceRequestInformation/OptionalQualifiers/PricingQualifiers/AlternativePricing/Overrides/NoMatch</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="CommandPricing" minOccurs="0" maxOccurs="unbounded">
                                                                            <annotation>
                                                                                <documentation>All of the CommandPricing entries are concerned with specifying fare basis codes, ticket designators, and discounts.  This base level command is WPQ followed by one or more of the below elements</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Discount" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify discount information</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Amount" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Amount</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="AuthCode" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Authorization code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Percent" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Percent</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="FareBasis" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to price the itinerary utilizing the fare basis code</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to instruct the system to price the itinerary utilizing the fare basis code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="TicketDesignator" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify a ticket designator</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="RPH" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to match up with "SegmentSelect".  These formats require a "*", i.e. requires a "/", i.e. WPS1*QY/AD75</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="CommissionContract" type="string" minOccurs="0" maxOccurs="4">
                                                                            <annotation>
                                                                                <documentation>Used to select specific contracts to process at a time of pricing</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="Corporate" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify a corporate ID to price with</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="ID" type="string" maxOccurs="4">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify a corporate ID to price with</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Force" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to price with corporate fares only</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Fare" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Please note that this functionality is restricted to Sabre's Asian joint venture customers.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Type" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify a fare type.  Acceptable values are "NL" for normal fare, "EX" for special fare, and 'IT" for inclusive tour</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="FareOptions" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Different fare options</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="ASLBreakdown" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to display the original base fare and GST (when applicable), along with new information displaying the ASL amounts and any applicable GST</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="ASLIgnore" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to ignore Adjusted Selling Level processing logic</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Excursion" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to price the itinerary with an excursion fare if applicable</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="HandlingFees" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to to display the original base fare and GST (when applicable), along with new information displaying all handling fees (with applicable GST) in the response</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Net" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to price the itinerary with a net fare if applicable</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Private" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to price the itinerary with a private fare if applicable</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Public" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to price the itinerary with a public fare if applicable</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="ItineraryOptions" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Pricing variables related to itinerary</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="BreakFare" minOccurs="0" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to break the fare at a specified segment</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Segment number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="ForceConnection" minOccurs="0" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to force a connection at a specified segment</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Segment number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="ForceStopOver" minOccurs="0" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to force a stopover at a specified segment</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Segment number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="OverrideBreak" minOccurs="0" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to override a fare break</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Segment number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="SegmentSelect" minOccurs="0" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to price specified itinerary segments</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Segment number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="EndNumber" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Segment end number in range</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="RPH" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to match up with ".../CommandPricing" or .../ItineraryOptions/GoverningCarrierOverride or .../SpecificFare</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="SideTrip" minOccurs="0">
                                                                                        <complexType>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Segment number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="EndNumber" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Segment end number in range</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="ManuallyAdjustedSellingLevel" minOccurs="0" maxOccurs="unbounded">
                                                                            <annotation>
                                                                                <documentation>Used to to specify the markup/down amount that's to be applied at time of pricing to create a manually Adjusted Selling Level fare</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="MarkDown" type="decimal">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to price the itinerary with a mark down amount</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="MarkUp" type="decimal">
                                                                                    <annotation>
                                                                                        <documentation>Used to instruct the system to price the itinerary with a mark up amount. MarkDown cannot be combined with MarkUp</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="NameSelect" minOccurs="0" maxOccurs="unbounded">
                                                                            <annotation>
                                                                                <documentation>Used to instruct the system to price the itinerary based upon a particular name field</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="NameNumber" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Name number</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="EndNameNumber" type="string">
                                                                                    <annotation>
                                                                                        <documentation>End number within a range</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="NetRemit" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>/NetRemit is ONLY applicable to BSP-based subscribers. /NetRemit is NOT applicable to ARC-based subscribers</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="CAR" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Car code</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Car code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Cash" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify the amount of net cash</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="CurrencyCode" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Currency code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Amount" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify the amount of net cash</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Credit" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify the amount of credit</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Amount" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify the amount of credit</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Code" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Net remit code</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Overrides" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Pricing overrides</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="FuelSurchargeOverride" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to override applicable fuel surcharges</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to instruct the system to override applicable fuel surcharges</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="GoverningCarrierOverride" minOccurs="0" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to override the governing carrier associated with the entire itinerary, a particular segment, or a range of segments</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="Airline">
                                                                                                    <annotation>
                                                                                                        <documentation>Airline code</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <attribute name="Code" type="string" use="required">
                                                                                                            <annotation>
                                                                                                                <documentation>Airline code</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                            <attribute name="RPH" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to tie a governing carrier override to particular segments. If a client application needs to utilize a governing carrier override from a particular segment or segments the application can use .../GoverningCarrierOverride in conjunction with .../ItineraryOptions/SegmentSelect</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="NoAdvancePurchase" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to price the itinerary with fares having no advance purchase requirements. Cannot combine with "NoFareRestrictions."</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to instruct the system to price the itinerary with fares having no advance purchase requirements</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="NoFareRestrictions" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to price the itinerary with fares having no restrictions. Cannot combine with "NoAdvancePurchase", "NoMinMaxStay", or "NoPenalty"</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to instruct the system to price the itinerary with fares having no restrictions</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="NoMinMaxStay" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to price the itinerary with fares having no minimum/maximum stay requirements. Cannot combine with "NoFareRestrictions"</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to instruct the system to price the itinerary with fares having no minimum/maximum stay requirements</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="NoPenalty" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to price the itinerary with fares having no penalties. Cannot combine with "NoFareRestrictions"</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to instruct the system to price the itinerary with fares having no penalties</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Purchase" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to override the default POS location, and price the itinerary as if the default POS location were located in the alternate, specified location</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="LocationCode" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Location code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Ticketing" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to instruct the system to override the default ticketing location, and price the itinerary as if the default ticketing location were located in the alternate, specified location</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="LocationCode" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Location code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="PassengerStatus" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to is used to instruct the system to price the itinerary with a specified passenger status code. Equivalent Sabre host command: WPEM/(country or state code), WPRY/(country or state code), WPNT/(country or state code)</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="PassengerType" minOccurs="0" maxOccurs="unbounded">
                                                                            <annotation>
                                                                                <documentation>Used to specify a passenger type code</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Code" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify a passenger type code</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Force" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to force the system to price with the specified passenger type code. If the client requests multiple passenger types and also wishes to make use of @Force they should only pass it on the last .../PassengerType occurrence</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Quantity" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the number of passengers</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="PlusUp" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify an amount to add on top of the fare</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="TicketDesignator" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Ticket designator</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Amount" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify an amount to add on top of the fare</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="RetailerRuleQualifier" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to price an itinerary incorporating fares linked to a specific Retailer Rule Qualifier code</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Qualifier" type="string" maxOccurs="4">
                                                                                        <annotation>
                                                                                            <documentation>Retailer Rule Qualifier code</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Force" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to force only fares with a matched Business Rule containing the specified Retailer Rule Qualifier. "Force" can only be used with one "Qualifier"</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="ShipsRegistry" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify a ship's country of registration. This element is only to be used when .../PassengerType@Code="SEA"</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="SpanishLargeFamilyDiscountLevel" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify the Spanish large family discount level. Please note that this qualifier is only applicable to Spain-based subscribers</documentation>
                                                                            </annotation>
                                                                            <simpleType>
                                                                                <restriction base="string">
                                                                                    <enumeration value="1"/>
                                                                                    <enumeration value="2"/>
                                                                                </restriction>
                                                                            </simpleType>
                                                                        </element>
                                                                        <element name="SpanishResidentDiscount" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify the Island Resident code applicable for discount.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="SpecificFare" minOccurs="0" maxOccurs="unbounded">
                                                                            <annotation>
                                                                                <documentation>This element is only applicable to Abacus</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="FareBasis" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to price a specific fare basis that will, in turn, be validated</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="RPH" type="string">
                                                                                    <annotation>
                                                                                        <documentation>RPH" is used to match up with "SegmentSelect"</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="SpecificPenalty" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify change or cancellation amounts for an itinerary</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Changeable" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Changeable pricing fare options. Please note that "Changeable" and "Refundable" can be combined in a single request to query for both changeable AND refundable pricing options</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Any" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify only changeable pricing fare options</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="CurrencyCode" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify the specific currency for maximum exchange penalty</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="MaxPenalty" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify maximum change penalty. If a value of "0" is input no penalty is preferred. If a value of "N" is input only non-changeable options are requested. Please note that monetary amount that includes a decimal is not allowed</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="BeforeDeparture" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to query for before departure fees</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="AfterDeparture" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to query for after departure fees. It is not possible to combine "BeforeDeparture" and "AfterDeparture" within one element</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="EitherOr" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Refundable OR changeable pricing fare options</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Any" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify any refundable OR changeable pricing fare options</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="CurrencyCode" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify the specific currency for either maximum refund OR change penalty</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="MaxPenalty" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify maximum refund OR change penalty. If a value of "0" is input no penalty is preferred. If a value of "N" is input only non-refundable OR non-changeable options are requested. Please note that monetary amount that includes a decimal is not allowed</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="BeforeDeparture" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to query for before departure fees</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="AfterDeparture" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to query for after departure fees. It is not possible to combine "BeforeDeparture" and "AfterDeparture" within one element</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Refundable" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Refundable pricing fare options. Please note that "Changeable" and "Refundable" can be combined in a single request to query for both changeable AND refundable pricing options</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Any" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify any refundable pricing fare options</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="CurrencyCode" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify the specific currency for maximum refund penalty</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="MaxPenalty" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify maximum refund penalty. If a value of "0" is input no penalty is preferred. If a value of "N" is input only non-refundable options are requested. Please note that monetary amount that includes a decimal is not allowed</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="BeforeDeparture" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to query for before departure fees</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="AfterDeparture" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to query for after departure fees. It is not possible to combine "BeforeDeparture" and "AfterDeparture" within one element</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="AdditionalInfo" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to receive additional tags with information on fare flexibility</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Taxes" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Tax related pricing variables</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="NoTax" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify to price an itinerary without taxes</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Ind" type="boolean" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify to price an itinerary without taxes</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="TaxExempt" minOccurs="0" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify a tax code to exempt</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify a tax code to exempt</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="TaxOverride" minOccurs="0" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify a tax to override</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Amount" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Tax amount</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Code" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Tax code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="ValidityDates" minOccurs="0" maxOccurs="unbounded">
                                                                            <annotation>
                                                                                <documentation>Used to specify ticket validity dates</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="NotValidAfter" type="spct:date" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify the ending validity date. "NotValidAfter" follows this format: MM-DD or YYYY-MM-DD</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="NotValidBefore" type="spct:date" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify the beginning validity date. "NotValidBefore" follows this format: MM-DD or YYYY-MM-DD</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Segment">
                                                                                        <annotation>
                                                                                            <documentation>Used to specify the relevant itinerary segments</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Segment number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="EndNumber" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Segment end number in range</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="AlternateCurrencyCode" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify an alternate currency code to price with</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="CurrencyCode" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify a currency code to price with</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="AggregatedContentExclude" type="boolean">
                                                                        <annotation>
                                                                            <documentation>Used to exclude aggregated content. Please note that it can only be combined with .../PriceRequestInformation/OptionalQualifiers/PricingQualifiers/BargainFinder</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="FareFocusExclude" type="boolean">
                                                                        <annotation>
                                                                            <documentation>Used to exclude or ignore fare focus fares</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="NoDate" type="boolean">
                                                                        <annotation>
                                                                            <documentation>Used to price itinerary without a date</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="RoundTheWorld" type="boolean">
                                                                        <annotation>
                                                                            <documentation>Allows a request for a round the world/circle trip fare</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="Source" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify the pricing source</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                            <attribute name="AlternativePricing" type="boolean">
                                                <annotation>
                                                    <documentation>Used to instruct the system to request Alternative Pricing. Please note that "AlternativePricing" cannot be combined with .../PriceRequestInformation/OptionalQualifiers/PricingQualifiers/BargainFinder</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="FutureTicket" type="boolean">
                                                <annotation>
                                                    <documentation>Used to instruct the system to create an FP line for future ticketing</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="Manual" type="boolean">
                                                <annotation>
                                                    <documentation>This attribute is not used</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="Retain" type="boolean">
                                                <annotation>
                                                    <documentation>Used to instruct the system to retain the pricing response and create a price quote record.</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                </sequence>
                            </complexType>
                        </element>
                        <element name="AirTax" minOccurs="0">
                            <annotation>
                                <documentation>Used to request air tax information</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element name="ItineraryInfos">
                                        <annotation>
                                            <documentation>Itinerary information</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="ItineraryInfo" maxOccurs="50">
                                                    <annotation>
                                                        <documentation>Itinerary information</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="ReservationItems">
                                                                <annotation>
                                                                    <documentation>Itinerary information</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="Item">
                                                                            <annotation>
                                                                                <documentation>Single itinerary</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="AirFareInfo">
                                                                                        <annotation>
                                                                                            <documentation>Fare details</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="PTC_FareBreakdown">
                                                                                                    <annotation>
                                                                                                        <documentation>Fare breakdown</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <sequence>
                                                                                                            <element name="FareBasisCode" minOccurs="0">
                                                                                                                <annotation>
                                                                                                                    <documentation>FareBasisCode information is needed for YQ/YR tax information</documentation>
                                                                                                                </annotation>
                                                                                                                <simpleType>
                                                                                                                    <restriction base="string">
                                                                                                                        <minLength value="1"/>
                                                                                                                        <maxLength value="15"/>
                                                                                                                        <pattern value="[A-Z0-9]+(/[A-Z0-9]+)?"/>
                                                                                                                    </restriction>
                                                                                                                </simpleType>
                                                                                                            </element>
                                                                                                            <element name="PassengerFare">
                                                                                                                <annotation>
                                                                                                                    <documentation>Passenger Fare</documentation>
                                                                                                                </annotation>
                                                                                                                <complexType>
                                                                                                                    <sequence>
                                                                                                                        <element name="BaseFare">
                                                                                                                            <annotation>
                                                                                                                                <documentation>Base fare</documentation>
                                                                                                                            </annotation>
                                                                                                                            <complexType>
                                                                                                                                <attribute name="Amount" type="decimal" use="required">
                                                                                                                                    <annotation>
                                                                                                                                        <documentation>Base fare amount "Amount" is required to calculate the percentage taxes</documentation>
                                                                                                                                    </annotation>
                                                                                                                                </attribute>
                                                                                                                                <attribute name="CurrencyCode" type="string">
                                                                                                                                    <annotation>
                                                                                                                                        <documentation>Currency code</documentation>
                                                                                                                                    </annotation>
                                                                                                                                </attribute>
                                                                                                                            </complexType>
                                                                                                                        </element>
                                                                                                                        <element name="EquivFare" minOccurs="0">
                                                                                                                            <annotation>
                                                                                                                                <documentation>Equivalent fare</documentation>
                                                                                                                            </annotation>
                                                                                                                            <complexType>
                                                                                                                                <attribute name="Amount" type="decimal">
                                                                                                                                    <annotation>
                                                                                                                                        <documentation>Equivalent fare amount</documentation>
                                                                                                                                    </annotation>
                                                                                                                                </attribute>
                                                                                                                                <attribute name="CurrencyCode" type="string">
                                                                                                                                    <annotation>
                                                                                                                                        <documentation>Currency code</documentation>
                                                                                                                                    </annotation>
                                                                                                                                </attribute>
                                                                                                                            </complexType>
                                                                                                                        </element>
                                                                                                                    </sequence>
                                                                                                                    <attribute name="AncillaryServiceCode" type="string">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Ancillary code</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                </complexType>
                                                                                                            </element>
                                                                                                            <element name="PassengerType">
                                                                                                                <annotation>
                                                                                                                    <documentation>Passenger information</documentation>
                                                                                                                </annotation>
                                                                                                                <complexType>
                                                                                                                    <attribute name="Quantity" type="integer">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Passenger type count</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                    <attribute name="Code" type="string" use="required">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Passenger type code</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                    <attribute name="Age" type="integer">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Passenger age</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                </complexType>
                                                                                                            </element>
                                                                                                        </sequence>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="FlightSegment" maxOccurs="unbounded">
                                                                                        <annotation>
                                                                                            <documentation>Air segment details</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="ArrivalAirport">
                                                                                                    <annotation>
                                                                                                        <documentation>Arrival airport</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <attribute name="LocationCode" type="string" use="required">
                                                                                                            <annotation>
                                                                                                                <documentation>Airport code</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                        <attribute name="CodeContext" type="string">
                                                                                                            <annotation>
                                                                                                                <documentation>Attribute is not in use</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                                <element name="DepartureAirport">
                                                                                                    <annotation>
                                                                                                        <documentation>Departure airport</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <attribute name="LocationCode" type="string" use="required">
                                                                                                            <annotation>
                                                                                                                <documentation>Airport code</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                        <attribute name="CodeContext" type="string">
                                                                                                            <annotation>
                                                                                                                <documentation>Attribute is not in use</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                                <element name="Equipment">
                                                                                                    <annotation>
                                                                                                        <documentation>Machine type </documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <attribute name="AirEquipType" type="string" use="required">
                                                                                                            <annotation>
                                                                                                                <documentation>Machine type code</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                                <element name="FareBreakInfo" minOccurs="0">
                                                                                                    <annotation>
                                                                                                        <documentation>Details on the specific fare break</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <sequence>
                                                                                                            <element name="BaseFare" minOccurs="0">
                                                                                                                <annotation>
                                                                                                                    <documentation>Base Fare. BaseFare element is needed only with the last flight segment in a fare break.</documentation>
                                                                                                                </annotation>
                                                                                                                <complexType>
                                                                                                                    <attribute name="Amount" type="decimal" use="required">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Amount</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                    <attribute name="CurrencyCode" type="string">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Currency code</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                </complexType>
                                                                                                            </element>
                                                                                                            <element name="EquivFare" minOccurs="0">
                                                                                                                <annotation>
                                                                                                                    <documentation>Equivalent fare. EquivalentFare element is needed only with the last flight segment in a fare break.</documentation>
                                                                                                                </annotation>
                                                                                                                <complexType>
                                                                                                                    <attribute name="Amount" type="decimal">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Amount</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                    <attribute name="CurrencyCode" type="string">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Currency code</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                </complexType>
                                                                                                            </element>
                                                                                                            <element name="FareBasisCode" minOccurs="0">
                                                                                                                <annotation>
                                                                                                                    <documentation>Fare basis code. FareBasisCode information is needed only with the last flight segment in a fare break.</documentation>
                                                                                                                </annotation>
                                                                                                                <simpleType>
                                                                                                                    <restriction base="string">
                                                                                                                        <minLength value="1"/>
                                                                                                                        <maxLength value="15"/>
                                                                                                                        <pattern value="[A-Z0-9]+(/[A-Z0-9]+)?"/>
                                                                                                                    </restriction>
                                                                                                                </simpleType>
                                                                                                            </element>
                                                                                                        </sequence>
                                                                                                        <attribute name="SideTripStartInd" type="boolean">
                                                                                                            <annotation>
                                                                                                                <documentation>Used to indicate if a flight segment is the start of a side trip.</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                        <attribute name="SideTripEndInd" type="boolean">
                                                                                                            <annotation>
                                                                                                                <documentation>Used to indicate if a flight segment is the end of a side trip.</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                        <attribute name="SideTripNumber" type="integer">
                                                                                                            <annotation>
                                                                                                                <documentation>Used to indicate the side trip number to which this flight segment belongs. If more than one side trips in the itinerary, identify side trip to which each flight segment belongs using the SideTripNumber.</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                        <attribute name="FareComponentNumber" type="integer" use="required">
                                                                                                            <annotation>
                                                                                                                <documentation>Used to indicate the fare component number to which the flight segment belongs. If more than one fare break is needed, identify the fare component to which each flight segment belongs using the FareComponentNumber.</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                                <element name="HiddenStops" minOccurs="0">
                                                                                                    <annotation>
                                                                                                        <documentation>Used to pass hidden city / via code</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <sequence>
                                                                                                            <element name="HiddenStop" maxOccurs="unbounded">
                                                                                                                <annotation>
                                                                                                                    <documentation>Used to pass hidden city / via code</documentation>
                                                                                                                </annotation>
                                                                                                                <complexType>
                                                                                                                    <attribute name="LocationCode" type="string" use="required">
                                                                                                                        <annotation>
                                                                                                                            <documentation>Airport code</documentation>
                                                                                                                        </annotation>
                                                                                                                    </attribute>
                                                                                                                </complexType>
                                                                                                            </element>
                                                                                                        </sequence>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                                <element name="MarketingAirline">
                                                                                                    <annotation>
                                                                                                        <documentation>Marketing airline code</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <attribute name="Code" type="string" use="required">
                                                                                                            <annotation>
                                                                                                                <documentation>Carrier code</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                                <element name="OperatingAirline" minOccurs="0">
                                                                                                    <annotation>
                                                                                                        <documentation>Operating airline code</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <attribute name="Code" type="string" use="required">
                                                                                                            <annotation>
                                                                                                                <documentation>Carrier code</documentation>
                                                                                                            </annotation>
                                                                                                        </attribute>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                            <attribute name="DepartureDateTime" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Departure Date and Time</documentation>
                                                                                                </annotation>
                                                                                                <simpleType>
                                                                                                    <restriction base="string">
                                                                                                        <pattern value="[2][0-9]{3}-[0-1][0-9]-[0-3][0-9][T][0-9]{2}:[0-9]{2}:[0-9]{2}"/>
                                                                                                    </restriction>
                                                                                                </simpleType>
                                                                                            </attribute>
                                                                                            <attribute name="ArrivalDateTime" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Arrival Date and Time</documentation>
                                                                                                </annotation>
                                                                                                <simpleType>
                                                                                                    <restriction base="string">
                                                                                                        <pattern value="[2][0-9]{3}-[0-1][0-9]-[0-3][0-9][T][0-9]{2}:[0-9]{2}:[0-9]{2}"/>
                                                                                                    </restriction>
                                                                                                </simpleType>
                                                                                            </attribute>
                                                                                            <attribute name="FlightNumber" type="integer" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Marketing Flight Number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="ResBookDesigCode" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Booking Code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="ForceConnectionInd" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to indicate whether the Flight segment is a forced connection.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="ForceStopOverInd" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to indicate whether the Flight segment is a forced stop over.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="RPH" type="integer" use="required">
                                                                                    <annotation>
                                                                                        <documentation>"RPH" is used to pass Sequence Number of the itineraries. For example, 1 for itinerary #1 and 2 or itinerary #2.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="TicketingCarrier" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>"TicketingCarrier" is used to pass the Ticketing Carrier.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="ValidatingCarrier" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>"ValidatingCarrier" is used to pass the Validating Carrier.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="SalePseudoCityCode" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>If "SalePseudoCityCode" is an invalid Sabre PCC, no tax processing will be done and an error response will be sent back</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="POS" minOccurs="0">
                                        <annotation>
                                            <documentation>Point of Sale details</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="Source" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Point of Sale details</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="TPA_Extensions" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Additional POS details</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="UserInfo">
                                                                            <annotation>
                                                                                <documentation>Additional POS details</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="AAACity">
                                                                                        <annotation>
                                                                                            <documentation> AAA City</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation> AAA City</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Action" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Action Code</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Action Code</documentation>
                                                                                                </annotation>
                                                                                                <simpleType>
                                                                                                    <restriction base="string">
                                                                                                        <minLength value="1"/>
                                                                                                        <maxLength value="5"/>
                                                                                                    </restriction>
                                                                                                </simpleType>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="AgentSine">
                                                                                        <annotation>
                                                                                            <documentation>Agent Sine</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Agent Sine</documentation>
                                                                                                </annotation>
                                                                                                <simpleType>
                                                                                                    <restriction base="string">
                                                                                                        <maxLength value="3"/>
                                                                                                    </restriction>
                                                                                                </simpleType>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Branch" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>User Branch</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" type="integer" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>User Branch</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Partition">
                                                                                        <annotation>
                                                                                            <documentation>Partition ID</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="ID" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Partition ID</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Service">
                                                                                        <annotation>
                                                                                            <documentation>Service Name</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Name" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Service Name</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="SetAddress" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>User set address</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Station" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>User Station</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" type="integer" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>User Station</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="Transaction" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Transaction ID</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="ID" type="integer" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Transaction ID</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                        <attribute name="PseudoCityCode" type="string">
                                                            <annotation>
                                                                <documentation>Sale PCC</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                </sequence>
                            </complexType>
                        </element>
                        <element name="HotelBook" minOccurs="0">
                            <annotation>
                                <documentation>Used to book GDS or Aggregator chain hotels.
                                    Please note that for the successful PNR creation TravelItineraryAddInfo.AgencyInfo.Address and TravelItineraryAddInfo.CustomerInfo.PersonName need to be provided.</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element name="BookingInfo">
                                        <annotation>
                                            <documentation>Used to pass booking information details.</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="BookingKey">
                                                    <annotation>
                                                        <documentation>The Booking Key is returned in the Hotel Price Check API response and is a mandatory value to perform hotel booking.
                                                            BookingKey cannot be empty and must not exceed 240 characters.</documentation>
                                                    </annotation>
                                                    <simpleType>
                                                        <restriction base="string">
                                                            <minLength value="1"/>
                                                        </restriction>
                                                    </simpleType>
                                                </element>
                                            </sequence>
                                            <attribute name="CorpDiscount">
                                                <annotation>
                                                    <documentation>A code that a company may give to an agency to use for a discount.
                                                        It is quite often tied to a negotiated rate code.</documentation>
                                                </annotation>
                                                <simpleType>
                                                    <restriction base="string">
                                                        <minLength value="1"/>
                                                    </restriction>
                                                </simpleType>
                                            </attribute>
                                            <attribute name="RequestorID" type="string">
                                                <annotation>
                                                    <documentation>Corresponds to the Agency IATA number.</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="Rooms" minOccurs="0">
                                        <annotation>
                                            <documentation>Room details.
                                                Multiple room bookings are currently not supported by GDS hotels.</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="Room" maxOccurs="99">
                                                    <annotation>
                                                        <documentation>Used to pass details of a single room.</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="Guests">
                                                                <annotation>
                                                                    <documentation>A list of guests in a room.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="Guest" maxOccurs="99">
                                                                            <annotation>
                                                                                <documentation>Used to pass details of a guest.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Contact" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Guest contact details</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Phone" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>The main phone number.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Mobile" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>The mobile phone number.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Fax" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>The Fax number.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Type">
                                                                                    <annotation>
                                                                                        <documentation>''Type''  defines whether the guest is an adult or child.
                                                                                            `10`- Adult.
                                                                                            `8` - Child.
                                                                                            Valid values are only Adult and Child Types.</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="int">
                                                                                            <enumeration value="8"/>
                                                                                            <enumeration value="10"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="Email">
                                                                                    <annotation>
                                                                                        <documentation>Email address for the guest</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="string">
                                                                                            <minLength value="1"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="Age">
                                                                                    <annotation>
                                                                                        <documentation>Used to pass the guest age.
                                                                                            It is mandatory for a child guest.</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="int">
                                                                                            <minInclusive value="1"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="Index">
                                                                                    <annotation>
                                                                                        <documentation>Used to pass the guest number.</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="int">
                                                                                            <minInclusive value="1"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="LeadGuest" type="boolean" default="false">
                                                                                    <annotation>
                                                                                        <documentation>Indicates that this guest is the main guest for the booking.
                                                                                            At least one and only one adult guest must be defined.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="FrequentFlyerNumber">
                                                                                    <annotation>
                                                                                        <documentation>Used to pass a frequent traveler number.
                                                                                            It applies only to the LeadGuest and should be specified as a two-letter airline code followed by the travelers frequent flyer number (eg. LH12345).</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="string">
                                                                                            <minLength value="1"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="LoyaltyId">
                                                                                    <annotation>
                                                                                        <documentation>Used to pass a loyalty identifier.
                                                                                            It applies only to the LeadGuest.</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="string">
                                                                                            <minLength value="1"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="Title" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to pass the guest title.
                                                                                            It can include MR./MS. etc.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="FirstName" type="string">
                                                                                    <annotation>
                                                                                        <documentation>First name of the guest.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="LastName" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Last name of the guest.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="NameNumber" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to associate the GDS legacy booking (`bookGDSviaCSL`=`false`) with a specific name number from the PNR name field.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                            <element name="RoomExtras" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to pass room extra information.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="RoomExtra" minOccurs="0" maxOccurs="99">
                                                                            <annotation>
                                                                                <documentation>Details of an individual extra requested.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Type">
                                                                                    <annotation>
                                                                                        <documentation>Used to pass the room extra type.
                                                                                            Currently supported types are:
                                                                                            `26` - Crib.
                                                                                            `91` - Roll-away Bed.
                                                                                            `196` - Extra Person.</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="int">
                                                                                            <minInclusive value="1"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="Quantity" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to pass the room extra quantity.
                                                                                            Mandatory for request.</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="int">
                                                                                            <minInclusive value="1"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="Amount">
                                                                                    <annotation>
                                                                                        <documentation>Used to pass the monetary amount of the room extra.</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="decimal">
                                                                                            <fractionDigits value="2"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                        <attribute name="NonSmoking" type="boolean" default="true">
                                                            <annotation>
                                                                <documentation>A non-smoking indicator.
                                                                    By default this is 'true'.
                                                                    When 'false' equals smoking and 'true' equals non-smoking.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="BedTypeCode">
                                                            <annotation>
                                                                <documentation>Allows users to request a specific bed type code.
                                                                    Used by EAN aggregator.</documentation>
                                                            </annotation>
                                                            <simpleType>
                                                                <restriction base="int">
                                                                    <minInclusive value="1"/>
                                                                </restriction>
                                                            </simpleType>
                                                        </attribute>
                                                        <attribute name="AccessibilityInformation">
                                                            <annotation>
                                                                <documentation>Used to allow a customer to request special accessibility rooms.</documentation>
                                                            </annotation>
                                                            <simpleType>
                                                                <restriction base="int">
                                                                    <minInclusive value="1"/>
                                                                </restriction>
                                                            </simpleType>
                                                        </attribute>
                                                        <attribute name="RoomIndex">
                                                            <annotation>
                                                                <documentation>Identifies the number of rooms being booked.
                                                                    For multi room bookings please specify '1' for the first room, '2' for the second room, etc.</documentation>
                                                            </annotation>
                                                            <simpleType>
                                                                <restriction base="int">
                                                                    <minInclusive value="1"/>
                                                                </restriction>
                                                            </simpleType>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                            <attribute name="NumberOfRooms" type="int">
                                                <annotation>
                                                    <documentation>Indicates the number of rooms to be booked.</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="SpecialInstructions" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to specify miscellaneous special preference information.
                                                All of the 'SpecialInstructions' should be free form or GTA special instructions.</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="SpecialInstruction" type="string" maxOccurs="99">
                                                    <annotation>
                                                        <documentation>Used to specify a single special instruction.</documentation>
                                                    </annotation>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="PaymentInformation" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to pass payment information for the hotel booking.
                                                Omit `PaymentInformation` entirely to pay later.
                                                In the event the chosen hotel property does not support this, an error will be returned.</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="FormOfPayment">
                                                    <annotation>
                                                        <documentation>Contains different forms of payment to pass for a hotel booking.
                                                            Only one may be used at a time.</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="PaymentCard" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to pass the details of a payment card.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="PaymentType">
                                                                            <annotation>
                                                                                <documentation>Used to pass the payment type.
                                                                                    Acceptable values include:
                                                                                    `CC` - credit card.
                                                                                    `CA` - cash.
                                                                                    `CK` - check.</documentation>
                                                                            </annotation>
                                                                            <simpleType>
                                                                                <restriction base="string">
                                                                                    <enumeration value="CC"/>
                                                                                    <enumeration value="CA"/>
                                                                                    <enumeration value="CK"/>
                                                                                    <enumeration value="MS"/>
                                                                                    <enumeration value="GR"/>
                                                                                    <enumeration value="SGR"/>
                                                                                </restriction>
                                                                            </simpleType>
                                                                        </element>
                                                                        <element name="CardCode">
                                                                            <annotation>
                                                                                <documentation>The 2 character code of the credit card issuer.</documentation>
                                                                            </annotation>
                                                                            <simpleType>
                                                                                <restriction base="string">
                                                                                    <length value="2"/>
                                                                                </restriction>
                                                                            </simpleType>
                                                                        </element>
                                                                        <element name="CardNumber" type="string">
                                                                            <annotation>
                                                                                <documentation>The credit card number.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="ExpiryMonth">
                                                                            <annotation>
                                                                                <documentation>The credit card expiration month.</documentation>
                                                                            </annotation>
                                                                            <simpleType>
                                                                                <restriction base="int">
                                                                                    <minInclusive value="1"/>
                                                                                    <maxInclusive value="12"/>
                                                                                </restriction>
                                                                            </simpleType>
                                                                        </element>
                                                                        <element name="ExpiryYear" type="gYear">
                                                                            <annotation>
                                                                                <documentation>The 4 digit credit card expiration year.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="FullCardHolderName" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to pass card holder name details.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Title" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The card holder name title.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="FirstName" type="string">
                                                                                        <annotation>
                                                                                            <documentation>The card holder first name.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="LastName" type="string">
                                                                                        <annotation>
                                                                                            <documentation>The card holder last name.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Email" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Card holder email address.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="CSC" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>The card security code.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="Address" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>The card holder address.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="AddressLine" type="string" minOccurs="0" maxOccurs="9">
                                                                                        <annotation>
                                                                                            <documentation>Used to pass the address information of the card holder.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="CityName" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The card holder address city name.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="CityCodes" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The card holder city codes.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="Code" maxOccurs="unbounded">
                                                                                                    <annotation>
                                                                                                        <documentation>The card holder city code.</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <simpleContent>
                                                                                                            <extension base="string">
                                                                                                                <attribute name="codeContext">
                                                                                                                    <annotation>
                                                                                                                        <documentation>Used to pass the source authority of the code.
                                                                                                                            Usually it is `IATA`.</documentation>
                                                                                                                    </annotation>
                                                                                                                </attribute>
                                                                                                            </extension>
                                                                                                        </simpleContent>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="StateProvince" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The card holder state or province information.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="code" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>The card holder state or province code.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="StateProvinceCodes" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to pass multiple state or province codes.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="Code" maxOccurs="99">
                                                                                                    <annotation>
                                                                                                        <documentation>The card holder state or province code.</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <simpleContent>
                                                                                                            <extension base="string">
                                                                                                                <attribute name="codeContext">
                                                                                                                    <annotation>
                                                                                                                        <documentation>Used to pass the source authority of the code.
                                                                                                                            Usually it is `IATA`.</documentation>
                                                                                                                    </annotation>
                                                                                                                </attribute>
                                                                                                            </extension>
                                                                                                        </simpleContent>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="PostCode" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The card holder postal code.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Country" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The card holder country information.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="code" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>The card holder country code.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="CountryCodes" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to pass multiple country codes.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="Code" maxOccurs="99">
                                                                                                    <annotation>
                                                                                                        <documentation>The card holder country code.</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <simpleContent>
                                                                                                            <extension base="string">
                                                                                                                <attribute name="codeContext">
                                                                                                                    <annotation>
                                                                                                                        <documentation>Used to pass the source authority of the code.
                                                                                                                            Usually it is `IATA`.</documentation>
                                                                                                                    </annotation>
                                                                                                                </attribute>
                                                                                                            </extension>
                                                                                                        </simpleContent>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Phone" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>The card holder phone details.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="PhoneUseType" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The card holder phone use type.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="PhoneCountryCode" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The card holder phone country code.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="PhoneNumber" type="string">
                                                                                        <annotation>
                                                                                            <documentation>The card holder phone number.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                            <element name="VirtualCard" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>The virtual payment card details.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="CustomerAccountCode" type="string">
                                                                            <annotation>
                                                                                <documentation>The customer account code.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="Agency">
                                                                            <complexType>
                                                                                <attribute name="Email" use="required">
                                                                                    <simpleType>
                                                                                        <restriction base="string">
                                                                                            <pattern value="[^@]+@[^\.]+\..+"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="HotelInfo">
                                                                            <complexType>
                                                                                <attribute name="Fax" type="string" use="required"/>
                                                                                <attribute name="HotelName" type="string" use="required"/>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="RoomDescription" maxOccurs="10">
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Text" type="string"/>
                                                                                </sequence>
                                                                                <attribute name="Name" type="string" use="required"/>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="RateInfo" minOccurs="0">
                                                                            <complexType>
                                                                                <attribute name="AmountAfterTax" type="decimal">
                                                                                    <annotation>
                                                                                        <documentation>Total amount for the booking. Please pass the value obtained from HotelPriceCheckRS/PriceCheckInfo/HotelRateInfo/RateInfos/RateInfo/@AmountAfterTax</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="CurrencyCode" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Currency code. Please pass the value obtained from HotelPriceCheckRS/PriceCheckInfo/HotelRateInfo/RateInfos/RateInfo/@CurrencyCode</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="SpecialInstructions">
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="SpecialInstruction" maxOccurs="10" type="string">
                                                                                        <annotation>
                                                                                            <documentation>
                                                                                                Used to inform the hotel about any extra charges that can be covered by the virtual card.
                                                                                                If the card is used to cover room only please pass "Room Only" value.
                                                                                            </documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                            <element name="Agency" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to pass agency form of payment information.
                                                                        There are two options for Agency form of payment:
                                                                        `1.` - Both 'Name' and 'Address'.
                                                                        `2.` - Only the 'IATANumber'.</documentation>
                                                                </annotation>

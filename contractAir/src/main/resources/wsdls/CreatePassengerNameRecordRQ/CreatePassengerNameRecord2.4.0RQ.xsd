                                                                    <sequence>
                                                                        <element name="Name" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>The agency name.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="Address" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>The agency address information.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="AddressLine" type="string" minOccurs="0" maxOccurs="9">
                                                                                        <annotation>
                                                                                            <documentation>The address information of the agency.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="CityName" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The agency address city name.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="StateProvince" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The agency state or province information.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="code" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>The agency state or province code.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="StateProvinceCodes" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to pass multiple state or province codes.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="Code" maxOccurs="99">
                                                                                                    <annotation>
                                                                                                        <documentation>The agency state or province code.</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <simpleContent>
                                                                                                            <extension base="string">
                                                                                                                <attribute name="codeContext">
                                                                                                                    <annotation>
                                                                                                                        <documentation>Used to pass the source authority of the code.
                                                                                                                            Usually it is `IATA`.</documentation>
                                                                                                                    </annotation>
                                                                                                                </attribute>
                                                                                                            </extension>
                                                                                                        </simpleContent>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="PostCode" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The agency postal code.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Country" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The agency country information.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="code" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>The agency country code.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="CountryCodes" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Used to pass multiple country codes.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="Code" maxOccurs="99">
                                                                                                    <annotation>
                                                                                                        <documentation>The agency country code.</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <simpleContent>
                                                                                                            <extension base="string">
                                                                                                                <attribute name="codeContext">
                                                                                                                    <annotation>
                                                                                                                        <documentation>Used to pass the source authority of the code.
                                                                                                                            Usually it is `IATA`.</documentation>
                                                                                                                    </annotation>
                                                                                                                </attribute>
                                                                                                            </extension>
                                                                                                        </simpleContent>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="IATANumber" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>The agency IATA number.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                            <element name="Corporate" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>The Corporate form of payment details.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="CorporateId" type="string">
                                                                            <annotation>
                                                                                <documentation>The corporate identifier.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                            <element name="Company" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to pass the company form of payment details.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="Name" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>The company name.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="Address" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>The company address.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="AddressLine" type="string" minOccurs="0" maxOccurs="9">
                                                                                        <annotation>
                                                                                            <documentation>The address information of the company.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="CityName" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The company address city name.</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="CityCodes" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The company city codes.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="Code" maxOccurs="unbounded">
                                                                                                    <annotation>
                                                                                                        <documentation>The company city code.</documentation>
                                                                                                    </annotation>
                                                                                                    <complexType>
                                                                                                        <simpleContent>
                                                                                                            <extension base="string">
                                                                                                                <attribute name="codeContext">
                                                                                                                    <annotation>
                                                                                                                        <documentation>Used to pass the source authority of the code.
                                                                                                                            Usually it is `IATA`.</documentation>
                                                                                                                    </annotation>
                                                                                                                </attribute>
                                                                                                            </extension>
                                                                                                        </simpleContent>
                                                                                                    </complexType>
                                                                                                </element>
                                                                                            </sequence>
                                                                                        </complexType>
                                                                                    </element>
                                                                                    <element name="StateProvince" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>The company state or province information.</documentation>
                                                                                        </annotation>

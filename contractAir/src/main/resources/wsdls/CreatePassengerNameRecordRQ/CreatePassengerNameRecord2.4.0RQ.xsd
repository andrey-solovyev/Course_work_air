<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns:spct="http://services.sabre.com/sp/common/simple/v3" xmlns:stlp="http://services.sabre.com/STL_Payload/v02_01" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:cpst="http://services.sabre.com/cp/common/simple/v1" xmlns:rct="http://services.sabre.com/sp/reservation/common/types/v1" xmlns:xsd="http://services.sabre.com/STL_MessageCommon/v02_01"  targetNamespace="http://services.sabre.com/sp/reservation/v2_4" elementFormDefault="qualified">
    <import namespace="http://services.sabre.com/STL_MessageCommon/v02_01" schemaLocation="built-ins/STL2_MsgCommon_v02_01.xsd"/>
    <import namespace="http://services.sabre.com/STL_Payload/v02_01" schemaLocation="built-ins/STL2_Payload_v02_01.xsd"/>
    <import namespace="http://services.sabre.com/sp/common/simple/v3" schemaLocation="built-ins/SpCommonSimpleTypes_v3.xsd"/>
    <import namespace="http://services.sabre.com/cp/common/simple/v1" schemaLocation="CommonSimpleTypes_v1.xsd"/>
    <import namespace="http://services.sabre.com/sp/reservation/common/types/v1" schemaLocation="CreatePassengerNameRecordCommonTypes1.0.0.xsd"/>
    <element name="CreatePassengerNameRecordRQ">
        <complexType>
            <complexContent>
                <extension base="stlp:STL_Request_Payload">
                    <sequence>
                        <element name="Profile" minOccurs="0">
                            <annotation>
                                <documentation>Used to add profile information</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element name="UniqueID">
                                        <annotation>
                                            <documentation>Used to pass profile ID </documentation>
                                        </annotation>
                                        <complexType>
                                            <attribute name="id" type="string" use="required">
                                                <annotation>
                                                    <documentation>Used to specify a profile name that is to be moved into the AAA to create the PNR</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                </sequence>
                            </complexType>
                        </element>
                        <element name="TravelItineraryAddInfo" minOccurs="0">
                            <annotation>
                                <documentation>Used to add passenger/agency information</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element name="AgencyInfo" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to pass agency details</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="Address" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Used to pass agency address</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="AddressLine" type="string" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>agency address</documentation>
                                                                </annotation>
                                                            </element>
                                                            <element name="CityName" type="string">
                                                                <annotation>
                                                                    <documentation>agency city name</documentation>
                                                                </annotation>
                                                            </element>
                                                            <element name="CountryCode" type="string" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>agency country code</documentation>
                                                                </annotation>
                                                            </element>
                                                            <element name="PostalCode" type="string" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>agency postal code</documentation>
                                                                </annotation>
                                                            </element>
                                                            <element name="StateCountyProv" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>state details</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="StateCode" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>state code</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="StreetNmbr" type="string" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>street number</documentation>
                                                                </annotation>
                                                            </element>
                                                            <element name="VendorPrefs" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to specify if the request is based upon a hosted or a non-hosted carrier.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="Airline">
                                                                            <annotation>
                                                                                <documentation>Specify if the request is based upon a hosted or a non-hosted carrier.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Hosted" type="boolean" use="required">
                                                                                    <annotation>
                                                                                        <documentation>For Travel Network customers "Hosted" should be set to "false" For SabreSonic RES hosted customers "Hosted" should be set to "true"</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                                <element name="Ticketing" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Used to pass ticketing related information</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <attribute name="PseudoCityCode" type="string">
                                                            <annotation>
                                                                <documentation>pseudo city code</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="QueueNumber" type="integer">
                                                            <annotation>
                                                                <documentation>queue number</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="QueueName" type="string">
                                                            <annotation>
                                                                <documentation>queue name</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="ShortText" type="string">
                                                            <annotation>
                                                                <documentation>miscellaneous free text</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="TicketTimeLimit" type="spct:dateTime">
                                                            <annotation>
                                                                <documentation>ticketing time limit, follows this format: MM-DDTHH:00, must be at the top of the hour </documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="TicketType" type="string" use="required">
                                                            <annotation>
                                                                <documentation>acceptable values are: "7TAW", "7TAX", "7T-A", 8(time), or 8(time/(date)</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="CustomerInfo" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to pass passenger details</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="ContactNumbers" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Used to pass passenger contact numbers</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="ContactNumber" maxOccurs="unbounded">
                                                                <annotation>
                                                                    <documentation>Used to pass contact details for a specific passenger</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="InsertAfter" type="integer">
                                                                        <annotation>
                                                                            <documentation>Used to add a phone number after or before an existing phone number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="LocationCode" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to override the default city in the application's Travel Journal Record</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="NameNumber" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify a passenger name number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="Phone" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to add customer telephone numbers into the record if applicable</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="PhoneUseType" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify if the number is agency "A", home "H", business "B" or fax "F".</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                                <element name="Corporate" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Used to pass corporate ID into the record if applicable. Cannot combine with any other qualifier</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="ID" type="string">
                                                                <annotation>
                                                                    <documentation>Used to add a corporate ID into the record if applicable.  Please note that this qualifier is not applicable to Sabre Sonic Res-based clients.  Furthermore, Sabre Travel Network-based customers need to first have this functionality activated for their particular location by their account representative prior to attempting to utilize this element</documentation>
                                                                </annotation>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                                <element name="CreditCardData" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Used to pass the credit card billing information. Please note that .../BillingInformation and .../PreferredCustomer are mutually exclusive</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <choice>
                                                            <element name="BillingInformation">
                                                                <annotation>
                                                                    <documentation>Used to pass the credit card billing information</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <choice>
                                                                        <element name="PostalCode" type="rct:shortTrimmedString">
                                                                            <annotation>
                                                                                <documentation>Used to specify the postal code of the card holder</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="Zip" type="rct:shortTrimmedString">
                                                                            <annotation>
                                                                                <documentation>Used to specify the zip code of the card holder</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                    </choice>
                                                                    <attribute name="cardHolderName" type="rct:shortTrimmedString" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the name of the card holder</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="streetAddress" type="rct:shortTrimmedString" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the street address of the card holder</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="city" type="rct:shortTrimmedString" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the city of the card holder</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="stateOrProvince" type="rct:shortTrimmedString">
                                                                        <annotation>
                                                                            <documentation>Used to specify the state or province of the card holder. "stateOrProvince" with a state code is required for addresses in the United States, U.S. Virgin Islands, Puerto Rico. "stateOrProvince" with a province code is required for addresses outside the United States, U.S. Virgin Islands, Puerto Rico</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="PreferredCustomer">
                                                                <complexType>
                                                                    <attribute name="ind" type="boolean" default="false">
                                                                        <annotation>
                                                                            <documentation>Used to indicate in a client's Passenger Name Record (PNR) that your client is a preferred customer, overriding the automatic address verification (AVS) check at time of ticketing</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </choice>
                                                    </complexType>
                                                </element>
                                                <element name="CustLoyalty" minOccurs="0" maxOccurs="unbounded">
                                                    <annotation>
                                                        <documentation>Used to pass frequent flyer number information</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <attribute name="MembershipID" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to add a frequent traveler number into the record.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="NameNumber" type="string">
                                                            <annotation>
                                                                <documentation>Used to specify a passenger name number.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="ProgramID" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to add the carrier code associated with the frequent traveler number</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="SegmentNumber" type="string">
                                                            <annotation>
                                                                <documentation>Used to add a segment number to associate the frequent traveler number to. SegmentNumber and TravelingCarrierCode cannot combine</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="TravelingCarrierCode" type="string">
                                                            <annotation>
                                                                <documentation>Used to transmit the frequent traveler number of one airline to accrue mileage on another carrier. SegmentNumber and TravelingCarrierCode cannot combine</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                                <element name="CustomerIdentifier" type="string" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Used to add a customer number (Also known as `DK number`) into the record.
                                                            Please note that this qualifier is not applicable to Sabre Sonic Res-based clients.
                                                            The value of can be `6`, `7`, or `10` alphanumeric.</documentation>
                                                    </annotation>
                                                </element>
                                                <element name="Email" minOccurs="0" maxOccurs="unbounded">
                                                    <annotation>
                                                        <documentation>Used to pass e-mail details</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <attribute name="Address" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to add an email address into the record</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="LanguageOverride" type="string">
                                                            <annotation>
                                                                <documentation>Used to override the language in the agency's Travel Journal Record (TJR).</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="NameNumber" type="string">
                                                            <annotation>
                                                                <documentation>Used to specify a passenger name number</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="ShortText" type="string">
                                                            <annotation>
                                                                <documentation>Used to specify miscellaneous email address-related free text information if applicable</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="Type">
                                                            <annotation>
                                                                <documentation>Used to specify the type of email address.  Acceptable values for "Type" are: "BC" for blind copy, "CC" for carbon copy, "FR" for from, or "To" for to</documentation>
                                                            </annotation>
                                                            <simpleType>
                                                                <restriction base="string">
                                                                    <enumeration value="BC"/>
                                                                    <enumeration value="CC"/>
                                                                    <enumeration value="FR"/>
                                                                    <enumeration value="TO"/>
                                                                </restriction>
                                                            </simpleType>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                                <element name="PersonName" minOccurs="0" maxOccurs="unbounded">
                                                    <annotation>
                                                        <documentation>Used to add passenger names</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="GivenName" type="string" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Passenger first name</documentation>
                                                                </annotation>
                                                            </element>
                                                            <element name="GroupInfo" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Group details</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="VendorPrefs" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify if the request is based upon a hosted or a non-hosted carrier.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Airline">
                                                                                        <annotation>
                                                                                            <documentation>Specify if the request is based upon a hosted or a non-hosted carrier.</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify the airline that the group is travelling on if applicable.  Please note that this qualifier is not applicable to Sabre Sonic Res-based subscribers, or Sabre Travel Network-based subscribers.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="Name" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify miscellaneous group booking-related free text title information if applicable</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="NumSeatsRemaining" type="integer" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the number of passengers travelling in a group if applicable</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="Surname" type="string" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Passenger surname</documentation>
                                                                </annotation>
                                                            </element>
                                                        </sequence>
                                                        <attribute name="Infant" type="boolean">
                                                            <annotation>
                                                                <documentation>Used to specify that the particular passenger is an infant</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="NameNumber" type="string">
                                                            <annotation>
                                                                <documentation>Used to specify a passenger name number, and is only applicable when used in conjunction with @PassengerType</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="NameReference" type="string">
                                                            <annotation>
                                                                <documentation>Used to specify miscellaneous name reference-related information if applicable</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="PassengerType" type="string">
                                                            <annotation>
                                                                <documentation>Used to add a passenger type code into the record.  Please note that this qualifier is not applicable to Sabre Sonic Res-based clients.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                </sequence>
                            </complexType>
                        </element>
                        <element name="AirBook" minOccurs="0">
                            <annotation>
                                <documentation>Used to add air segments</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element name="RetryRebook" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to specify if Rebooking of segments is required if UC is encountered</documentation>
                                        </annotation>
                                        <complexType>
                                            <attribute name="Option" type="boolean" use="required">
                                                <annotation>
                                                    <documentation>"Option" is used to specify if Rebooking of segments is required if UC is encountered in any of the segments which is trying to be booked</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="HaltOnStatus" minOccurs="0" maxOccurs="unbounded">
                                        <annotation>
                                            <documentation>Used to specify values that cause subsequent pricing, and beyond processing to stop if they appear</documentation>
                                        </annotation>
                                        <complexType>
                                            <attribute name="Code" use="required">
                                                <annotation>
                                                    <documentation>Used to specify values that cause subsequent pricing, and beyond processing to stop if they appear. In order to successfully price an air itinerary the status code associated with the individual segments cannot be NN, NO, UC or US. After successfully booking flights, the system will compare the status codes returned from the carrier against the client-specified codes to determine if subsequent pricing, and beyond processing should occur</documentation>
                                                </annotation>
                                                <simpleType>
                                                    <restriction base="string">
                                                        <minLength value="1"/>
                                                    </restriction>
                                                </simpleType>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="OriginDestinationInformation">
                                        <annotation>
                                            <documentation>Used to specify air segments to be booked</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="FlightSegment" maxOccurs="16">
                                                    <annotation>
                                                        <documentation>Used for a specific air segment to be booked.</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="DestinationLocation">
                                                                <annotation>
                                                                    <documentation>Used to specify the arrival airport code</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="LocationCode" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the arrival airport code</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="Equipment" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to specify the equipment type of the aircraft being booked</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="AirEquipType" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the equipment type of the aircraft being booked</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="MarketingAirline">
                                                                <annotation>
                                                                    <documentation>Used to specify the marketing airline code.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="Code" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the marketing airline code.</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="FlightNumber" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the flight number.  OPEN can also be passed if the user desires an open space ticket</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="MarriageGrp" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Information whether the flight segment is part of a married connection. O means no. I means yes.</documentation>
                                                                </annotation>
                                                                <simpleType>
                                                                    <restriction base="string">
                                                                        <enumeration value="O"/>
                                                                        <enumeration value="I"/>
                                                                    </restriction>
                                                                </simpleType>
                                                            </element>
                                                            <element name="OperatingAirline" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to specify the operating airline code</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="Code" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the operating airline code</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="OriginLocation">
                                                                <annotation>
                                                                    <documentation>Used to specify the departure airport code</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="LocationCode" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the departure airport code</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                        <attribute name="ArrivalDateTime" type="spct:dateTime">
                                                            <annotation>
                                                                <documentation>Used to specify the time and date of flight arrival.  Please note that the value within this attribute if passed cannot be null</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="DepartureDateTime" type="spct:dateTime">
                                                            <annotation>
                                                                <documentation>Used to specify the time and date of flight departure</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="FlightNumber" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to specify the flight number.  OPEN can also be passed if the user desires an open space ticket</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="NumberInParty" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to specify the number of passengers that need to be booked during this transaction</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="ResBookDesigCode" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to specify the booking class</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="Status" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to specify the action code to be used to sell the flight inventory. Valid values for "Status": NN, HL, GK, QF, SA, MM, or OPEN</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="InstantPurchase" type="boolean">
                                                            <annotation>
                                                                <documentation>Used to send a request to an open system AirBookRQ</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="AirlineRefId" type="string">
                                                            <annotation>
                                                                <documentation>Used to pass an airline reference for a booking made outside of Sabre system. When you combine "AirlineRefId"  with other flight segment details, system will create a passive PNR segment</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="RedisplayReservation" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to request a redisplay reservation attempt to validate if the desired segment status code remains unaltered</documentation>
                                        </annotation>
                                        <complexType>
                                            <attribute name="NumAttempts" use="required">
                                                <annotation>
                                                    <documentation>Used to specify a number of times to make the TravelItineraryReadRQ service call before proceeding to the OTA_AirPriceLLSRQ or the next service call. </documentation>
                                                </annotation>
                                                <simpleType>

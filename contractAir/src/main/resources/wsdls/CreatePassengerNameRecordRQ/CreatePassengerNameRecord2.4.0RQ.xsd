                                                <element name="Text" minOccurs="0" type="string">
                                                    <annotation>
                                                        <documentation>Used to pass miscellaneous special preference-related information if applicable.</documentation>
                                                    </annotation>
                                                </element>
                                            </sequence>
                                            <attribute name="WrittenConfirmation" type="boolean">
                                                <annotation>
                                                    <documentation>Used to request written rental confirmation from the vendor.</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="VehPrefs" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to request vehicle category.</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="VehPref">
                                                    <annotation>
                                                        <documentation>Used to specify the vehicle category if applicable.</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="VehType" type="string">
                                                                <annotation>
                                                                    <documentation>Used to specify the vehicle category if applicable.</documentation>
                                                                </annotation>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="VehRentalCore">
                                        <annotation>
                                            <documentation>Used to pass basic vehicle booking details.</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="PickUpLocation" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Used to specify the vehicle pick up location.</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="ArrivalFlightInfo" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to inform the vendor of the arrival flight number.</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="MarketingAirline">
                                                                            <annotation>
                                                                                <documentation>Used to inform the vendor of the arrival flight number.</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Code" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Carrier code</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="FlightNumber" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>The flight number</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                        <attribute name="LocationCode" type="string">
                                                            <annotation>
                                                                <documentation>The three-letter IATA airport code associated with the vehicle pick up location.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="ExtendedLocationCode" type="string">
                                                            <annotation>
                                                                <documentation>An identifier used when a car supplier has a rental location that is not present at an airport. If the extended location code is used, then it is displayed in 3 alpha, followed by 1 additional alpha and 2 numeric format. The additional 1 alpha value can be:  `N` - North, `S` - South, `E` - East, `W` - West, `R` - Resort, `C` - City, `P` - Port, `X` - Rail. Note: when  "ExtendedLocationCode" is specified, "LocationCode" also needs to be specified.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                                <element name="ReturnLocation" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Contains an alternate drop-off location, if applicable.</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <attribute name="LocationCode" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to specify an alternate drop-off location code, which can be a three-letter IATA city/airport location (eg. LON) or a 6 alpha-numeric for non-airport locations (eg. LONC18). When specifying a non-airport drop-off location the format is 6 alpha-numeric, constructed as: 3 alpha, followed by 1 additional alpha and 2 numeric format. The additional 1 alpha value can be: `N` - North, `S` - South, `E` - East, `W` - West, `R` - Resort, `C` - City, `P` - Port, `X` - Rail</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                            <attribute name="InsertAfter" type="integer">
                                                <annotation>
                                                    <documentation>Used to specify an existing segment to place the new car segment after.</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="PickUpDate" type="date">
                                                <annotation>
                                                    <documentation>Used to specify the rental pickup date, follows this format: YYYY-MM-DD</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="PickUpTime" type="spct:shortTime">
                                                <annotation>
                                                    <documentation>Used to specify the rental pickup time, follows this format: HH:MM</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="Quantity" type="integer">
                                                <annotation>
                                                    <documentation>Used to specify the number of vehicles being requested if applicable.</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="ReturnDate" type="date">
                                                <annotation>
                                                    <documentation>Used to specify the rental return date, follows this format: YYYY-MM-DD</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="ReturnTime" type="spct:shortTime">
                                                <annotation>
                                                    <documentation>Used to specify the rental return time, follows this format: HH:MM</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="VendorPrefs" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to specify vehicle vendor preferences.</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="VendorPref">
                                                    <annotation>
                                                        <documentation>Used to specify vehicle vendor preferences.</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <attribute name="Code" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to specify a vendor code.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                        <attribute name="Phone" type="string">
                                                            <annotation>
                                                                <documentation>Used to specify vendor phone number information if applicable.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                </sequence>
                            </complexType>
                        </element>
                        <element name="MiscSegment" minOccurs="0">
                            <annotation>
                                <documentation>Used to book miscellaneous segments such as Miscellaneous Charge Orders, Insurance Segments, Other product segments</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element name="OriginLocation">
                                        <annotation>
                                            <documentation>Used to specify the location to be associated with the miscellaneous segment.</documentation>
                                        </annotation>
                                        <complexType>
                                            <attribute name="LocationCode" type="string" use="required">
                                                <annotation>
                                                    <documentation>Location Code</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="Text" type="string">
                                        <annotation>
                                            <documentation>Used to specify free text information to be associated with the miscellaneous segment.</documentation>
                                        </annotation>
                                    </element>
                                    <element name="VendorPrefs">
                                        <annotation>
                                            <documentation>Used to specify a particular vendor code.</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="Airline">
                                                    <annotation>
                                                        <documentation>Airline code</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <attribute name="Code" type="string" use="required">
                                                            <annotation>
                                                                <documentation>Used to specify a particular vendor code.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                </sequence>
                                <attribute name="DepartureDateTime" type="spct:date" use="required">
                                    <annotation>
                                        <documentation>Used to specify the date associated with the miscellaneous segment.</documentation>
                                    </annotation>
                                </attribute>
                                <attribute name="InsertAfter" type="integer">
                                    <annotation>
                                        <documentation>Used to denote a segment after which the miscellaneous segment needs to be placed.</documentation>
                                    </annotation>
                                </attribute>
                                <attribute name="NumberInParty" type="integer" use="required">
                                    <annotation>
                                        <documentation>Used to specify the number of parties associated with the miscellaneous segment.</documentation>
                                    </annotation>
                                </attribute>
                                <attribute name="Status" type="string" use="required">
                                    <annotation>
                                        <documentation>Used to specify the action code to be used to sell the segment. Example: "GK"</documentation>
                                    </annotation>
                                </attribute>
                                <attribute name="Type" use="required">
                                    <annotation>
                                        <documentation>Used to specify the type of miscellaneous segment, i.e. OTH, MCO, PTA, or INS.</documentation>
                                    </annotation>
                                    <simpleType>
                                        <restriction base="string">
                                            <enumeration value="INS"/>
                                            <enumeration value="MCO"/>
                                            <enumeration value="OTH"/>
                                            <enumeration value="PTA"/>
                                        </restriction>
                                    </simpleType>
                                </attribute>
                            </complexType>
                        </element>
                        <element name="SpecialReqDetails" minOccurs="0">
                            <annotation>
                                <documentation>Used to add additional special requests such as seats, remarks, SSRs</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element name="AddRemark" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to add remarks</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="RemarkInfo">
                                                    <annotation>
                                                        <documentation>Details of the remark</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="FOP_Remark" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Form of payment remark</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="CC_Info" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Credit card details</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="PaymentCard">
                                                                                        <annotation>
                                                                                            <documentation>Card details</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="AirlineCode" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify an airline code to be used in conjunction with a credit card security code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="CardSecurityCode" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify a credit card security code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Code" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify a credit card vendor code</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="ExpireDate" type="spct:expireDate" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify a credit card expiration date</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="ExtendedPayment" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to define number of months for extended payment</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="ManualApprovalCode" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify a manual credit card if applicable</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Number" type="string" use="required">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify a credit card number</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="SuppressApprovalCode" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Used to specify to prevent the Sabre system from generating an approval code when the credit card number is input into the system.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Suppress" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify whether or not to suppress credit card information on an itinerary</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="Type" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify the form of payment type.</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="FutureQueuePlaceRemark" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used for future queue place remark</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="Date" type="spct:date" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify a queue placement date</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="PrefatoryInstructionCode" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the prefatory instruction code</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="PseudoCityCode" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify a pseudo city code</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="QueueIdentifier" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify a queue name/number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="Time" type="spct:time">
                                                                        <annotation>
                                                                            <documentation>Used to specify a queue placement time.  Please note that only whole hours can be specified, i.e. 12:00</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="Remark" minOccurs="0" maxOccurs="unbounded">
                                                                <annotation>
                                                                    <documentation>Used for multiple remark types</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="Text" type="string">
                                                                            <annotation>
                                                                                <documentation>Used to specify remark-related text.</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="Code" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify an alpha sorting character such as: "H"</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="SegmentNumber" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify a segment number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="Type" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify the type of remark.  Acceptable values are: Alpha-Coded, Client Address, Corporate, Delivery Address, General, Group Name, Hidden, Historical, Invoice, or Itinerary.</documentation>
                                                                        </annotation>
                                                                        <simpleType>
                                                                            <restriction base="string">
                                                                                <enumeration value="Alpha-Coded"/>
                                                                                <enumeration value="Client Address"/>
                                                                                <enumeration value="Corporate"/>
                                                                                <enumeration value="Delivery Address"/>
                                                                                <enumeration value="General"/>
                                                                                <enumeration value="Group Name"/>
                                                                                <enumeration value="Hidden"/>
                                                                                <enumeration value="Historical"/>
                                                                                <enumeration value="Invoice"/>
                                                                                <enumeration value="Itinerary"/>
                                                                            </restriction>
                                                                        </simpleType>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="AirSeat" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to book seats</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="Seats">
                                                    <annotation>
                                                        <documentation>Seat details</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="Seat" maxOccurs="98">
                                                                <annotation>
                                                                    <documentation>Single seat request</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="BoardingPass" type="boolean">
                                                                        <annotation>
                                                                            <documentation>Used to specify for the system to assign a seat and issue a boarding pass</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="ChangeOfGauge" type="boolean">
                                                                        <annotation>
                                                                            <documentation>Used to specify a change of gauge of flight.</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="NameNumber" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify a passenger name number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="Number" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify a seat number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="Preference" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify a seat preference. Preference cannot combine with Number.</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="SegmentNumber" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify a segment number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="SpecialService" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to add SSRs</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="SpecialServiceInfo">
                                                    <annotation>
                                                        <documentation>SSR details</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="AdvancePassenger" minOccurs="0" maxOccurs="unbounded">
                                                                <annotation>
                                                                    <documentation>Used to specify the APIS (Advance Passenger Information)</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="Document" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Document details</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="IssueCountry" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Issue country</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="NationalityCountry" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Nationality country</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Visa" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Visa document details</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <sequence>
                                                                                                <element name="ApplicableCountry" type="string" minOccurs="0">
                                                                                                    <annotation>
                                                                                                        <documentation>You must use a valid 2 or 3 character country code (alpha characters only)</documentation>
                                                                                                    </annotation>
                                                                                                </element>
                                                                                                <element name="PlaceOfBirth" type="string" minOccurs="0">
                                                                                                    <annotation>
                                                                                                        <documentation>Place of birth</documentation>
                                                                                                    </annotation>
                                                                                                </element>
                                                                                                <element name="PlaceOfIssue" type="string" minOccurs="0">
                                                                                                    <annotation>
                                                                                                        <documentation>Place of issue</documentation>
                                                                                                    </annotation>
                                                                                                </element>
                                                                                            </sequence>
                                                                                            <attribute name="IssueDate" type="spct:fullDate">
                                                                                                <annotation>
                                                                                                    <documentation>Issue date</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="ExpirationDate" type="spct:fullDate">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the document expiration date</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Number" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the document number.</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Type" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the document type. Acceptable values include: "A" - Alien resident card, "C" - Permanent resident card, "F" - Facilitation document, "I" - National ID card, "IN" - Nexus Card. Also: "M" - Military, "N" - Naturalization certificate, "P" - Passport, "T" - Refugee travel document and re-entry permit, US Travel document, "V" - Border crossing card</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="PersonName" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Personal details</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="GivenName" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>First name</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="MiddleName" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Middle name</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Surname" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Surname</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="DateOfBirth" type="spct:fullDate">
                                                                                    <annotation>
                                                                                        <documentation>Date of birth</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="DocumentHolder" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to identify the primary passport holder when the passport document is issued for multiple passengers</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Gender">
                                                                                    <annotation>
                                                                                        <documentation>Gender</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="string">
                                                                                            <enumeration value="F"/>
                                                                                            <enumeration value="M"/>
                                                                                            <enumeration value="FI"/>
                                                                                            <enumeration value="MI"/>
                                                                                            <enumeration value="U"/>
                                                                                            <enumeration value="X"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="LapChild" type="boolean">
                                                                                    <annotation>
                                                                                        <documentation>Used to identify information associated with a lap child.
                                                                                            It can only be used together with .../SpecialServiceInfo/AdvancePassenger/Document/Visa and .../SpecialServiceInfo/AdvancePassenger/ResidentDestinationAddress</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="NameNumber" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Name number: 1.1, 2.1. etc</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="ResidentDestinationAddress" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to pass residence or destination address</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="City" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>City name</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Country" type="string">
                                                                                        <annotation>
                                                                                            <documentation>Country code</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Street" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Street address</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="State" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>State</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Zip" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Zip or postal code</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="Type" type="string" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify the address type. Acceptable values are "R" - residence address, "D" - destination address</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="VendorPrefs" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Airline preferences</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Airline">
                                                                                        <annotation>
                                                                                            <documentation>Airline</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Hosted" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Defines if the receiving airline is Sabre hosted carrier</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="SegmentNumber" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify an itinerary segment to associate the SSR to.  "A" can be specified to associate the SSR to all segments.</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="SecureFlight" minOccurs="0" maxOccurs="unbounded">
                                                                <annotation>
                                                                    <documentation>Used to specify Secure Flight Passenger Data (SFPD)</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="IssueCountry" minOccurs="0" type="string">
                                                                            <annotation>
                                                                                <documentation>Used to pass country of issue for "KnownTravelerNumber" or "RedressNumber"</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="KnownTravelerNumber" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>"KnownTravelerNumber" cannot combine with ".../PersonName@DateOfBirth" or ".../PersonName@Gender" and "RedressNumber"</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="PersonName" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Personal details</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="GivenName" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>First name</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                    <element name="Surname" type="string" minOccurs="0">
                                                                                        <annotation>
                                                                                            <documentation>Surname</documentation>
                                                                                        </annotation>
                                                                                    </element>
                                                                                </sequence>
                                                                                <attribute name="DateOfBirth" type="string">
                                                                                    <annotation>
                                                                                        <documentation>"DateOfBirth" and "Gender" cannot combine with "RedressNumber" and "KnownTravelerNumber"</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Gender">
                                                                                    <annotation>
                                                                                        <documentation>Gender</documentation>
                                                                                    </annotation>
                                                                                    <simpleType>
                                                                                        <restriction base="string">
                                                                                            <enumeration value="F"/>
                                                                                            <enumeration value="M"/>
                                                                                            <enumeration value="FI"/>
                                                                                            <enumeration value="MI"/>
                                                                                            <enumeration value="U"/>
                                                                                            <enumeration value="X"/>
                                                                                        </restriction>
                                                                                    </simpleType>
                                                                                </attribute>
                                                                                <attribute name="NameNumber" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Name number: 1.1, 2.1. etc</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="RedressNumber" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>"RedressNumber" cannot combine with ".../PersonName@DateOfBirth" or ".../PersonName@Gender"</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="VendorPrefs" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Airline preferences</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Airline">
                                                                                        <annotation>
                                                                                            <documentation>Airline</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Hosted" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Defines if the receiving airline is Sabre hosted carrier</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="SegmentNumber" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify an itinerary segment to associate the SSR to.  "A" can be specified to associate the SSR to all segments</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="Service" minOccurs="0" maxOccurs="unbounded">
                                                                <annotation>
                                                                    <documentation>Used to specify Special Service Request (SSR)</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="PersonName" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Personal details</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="NameNumber" type="string">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify a passenger name number</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Text" type="string" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Used to specify miscellaneous free text information</documentation>
                                                                            </annotation>
                                                                        </element>
                                                                        <element name="VendorPrefs" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>Airline preferences</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <sequence>
                                                                                    <element name="Airline">
                                                                                        <annotation>
                                                                                            <documentation>Airline</documentation>
                                                                                        </annotation>
                                                                                        <complexType>
                                                                                            <attribute name="Code" type="string">
                                                                                                <annotation>
                                                                                                    <documentation>Applicable to the "OSI" SSRCode, and is used to specify the airline that the SSR needs to be sent to.</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                            <attribute name="Hosted" type="boolean">
                                                                                                <annotation>
                                                                                                    <documentation>Defines if the receiving airline is Sabre hosted carrier</documentation>
                                                                                                </annotation>
                                                                                            </attribute>
                                                                                        </complexType>
                                                                                    </element>
                                                                                </sequence>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="SegmentNumber" type="string">
                                                                        <annotation>
                                                                            <documentation>Used to specify an itinerary segment to associate the SSR to. If SSR_Code="OSI"  @SegmentNumber cannot be utilized.</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="SSR_Code" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to specify an SSR code such as "WCHR", "WGML", etc. </documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                </sequence>
                            </complexType>
                        </element>
                        <element name="PostProcessing">
                            <annotation>
                                <documentation>Additional variables used to finalize the PNR creation</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element name="ARUNK" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to add arrival unknown segments into a passenger name record.  The system will properly apply them so there is no need to segment select, etc...</documentation>
                                        </annotation>
                                        <complexType>
                                            <attribute name="priorPricing" type="boolean">
                                                <annotation>
                                                    <documentation>Used to add arrival unknown segments into a passenger name record after air segment sell but prior to pricing.</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="QueuePlace" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to request to place the newly created PNR on a queue</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="MultiQueuePlace" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Place on multiple queues</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="QueueIdentifier" maxOccurs="20">
                                                                <annotation>
                                                                    <documentation>Single queue details</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="Number" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Queue number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="PrefatoryInstructionCode" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>PIC code</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="PseudoCityCode" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>PCC </documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                                <element name="QueueInfo" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Place on a single queue</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="QueueIdentifier" maxOccurs="3">
                                                                <annotation>
                                                                    <documentation>Single queue details</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="Name" type="string">
                                                                        <annotation>
                                                                            <documentation>Queue name</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="Number" type="string">
                                                                        <annotation>
                                                                            <documentation>Queue number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="PrefatoryInstructionCode" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>PIC code</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                    <attribute name="PseudoCityCode" type="string">
                                                                        <annotation>
                                                                            <documentation>PCC</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="UniqueID" minOccurs="0" maxOccurs="6">
                                                                <annotation>
                                                                    <documentation>This element is not in use</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="ID" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>This attribute is not in use</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="EndTransaction">
                                        <annotation>
                                            <documentation>Used to finalize the transaction and commit the record</documentation>
                                        </annotation>
                                        <complexType>
                                            <sequence>
                                                <element name="Source">
                                                    <annotation>
                                                        <documentation>Used to identify the entity that authorized the changes in a Passenger Name Record</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <attribute name="ReceivedFrom" type="xsd:Text.Short" use="required">
                                                            <annotation>
                                                                <documentation>Source name</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                                <element name="Email" minOccurs="0">
                                                    <annotation>
                                                        <documentation>Used to request e-mail notifications after ending the record</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <sequence>
                                                            <element name="eTicket" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>eTicket related e-mail request</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="PDF" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>PDF request</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Ind" type="boolean" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to send an email notification containing a PDF-based copy of the eTicket</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="Ind" type="boolean" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to send an email notification containing a text-based copy of the eTicket</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="Invoice" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Invoice related e-mail request</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="Ind" type="boolean" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to send an email notification containing a text-based copy of the invoice</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="Itinerary" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Itinerary related e-mail request</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <sequence>
                                                                        <element name="PDF" minOccurs="0">
                                                                            <annotation>
                                                                                <documentation>PDF request</documentation>
                                                                            </annotation>
                                                                            <complexType>
                                                                                <attribute name="Ind" type="boolean" use="required">
                                                                                    <annotation>
                                                                                        <documentation>Used to send an email notification containing a PDF-based copy of the itinerary</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                            </complexType>
                                                                        </element>
                                                                        <element name="Segment" minOccurs="0" maxOccurs="unbounded">
                                                                            <complexType>
                                                                                <attribute name="EndNumber" type="integer">
                                                                                    <annotation>
                                                                                        <documentation>Used to specify an ending segment in the range to include in the email message</documentation>
                                                                                    </annotation>
                                                                                </attribute>
                                                                                <attribute name="Number" type="integer" use="required"/>
                                                                            </complexType>
                                                                        </element>
                                                                    </sequence>
                                                                    <attribute name="Ind" type="boolean" use="required">
                                                                        <annotation>
                                                                            <documentation>Used to send an email notification containing a text-based copy of the itinerary</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                            <element name="PersonName" minOccurs="0">
                                                                <annotation>
                                                                    <documentation>Used to specify a particular passenger name number to send the email notification to.  Please note that the email address in the PNR's email field must be name associated to the specified name number</documentation>
                                                                </annotation>
                                                                <complexType>
                                                                    <attribute name="NameNumber" type="string" use="required">
                                                                        <annotation>
                                                                            <documentation>Name number</documentation>
                                                                        </annotation>
                                                                    </attribute>
                                                                </complexType>
                                                            </element>
                                                        </sequence>
                                                        <attribute name="Ind" type="boolean" use="required">
                                                            <annotation>
                                                                <documentation>Used to send an email notification upon end transaction to any email addresses contained within the particular record</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                                <element name="ScheduleChange" minOccurs="0">
                                                    <annotation>
                                                        <documentation>''ScheduleChange'' is used to update schedule changes.</documentation>
                                                    </annotation>
                                                    <complexType>
                                                        <attribute name="Ind" type="boolean" use="required">
                                                            <annotation>
                                                                <documentation>''Ind'' is the schedule change indicator.</documentation>
                                                            </annotation>
                                                        </attribute>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="PostBookingHKValidation" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to specify wait interval to validate whether after booking is ended code share flights keep the holding confirmed status. If the "HK" status changes for any of the flight segments the service will pass corresponding warning message. The maximum time that can be set is 60 seconds. "PostBookingHKValidation" must be combined with .../PostProcessing/RedisplayReservation</documentation>
                                        </annotation>
                                        <complexType>
                                            <attribute name="waitInterval" type="rct:waitIntervalType" use="required">
                                                <annotation>
                                                    <documentation>The maximum time that can be set is 60 seconds. This applies to PostBookingHKValidation, WaitForAirlineRecLoc or both</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="numAttempts" type="rct:numAttemptsType" use="required">
                                                <annotation>
                                                    <documentation>Used to request a redisplay reservation number of attempts</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="WaitForAirlineRecLoc" minOccurs="0">
                                        <annotation>
                                            <documentation>Used to specify wait interval to wait for airline record locators to be returned. If the locators are not returned after the specified interval and number of attempts service will pass corresponding warning message. "WaitForAirlineRecLoc" must be combined with .../PostProcessing/RedisplayReservation</documentation>
                                        </annotation>
                                        <complexType>
                                            <attribute name="waitInterval" type="rct:waitIntervalType" use="required">
                                                <annotation>
                                                    <documentation>The maximum time that can be set is 60 seconds. This applies to PostBookingHKValidation, WaitForAirlineRecLoc or both</documentation>
                                                </annotation>
                                            </attribute>
                                            <attribute name="numAttempts" type="rct:numAttemptsType" use="required">
                                                <annotation>
                                                    <documentation>Used to request a redisplay reservation number of attempts</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                    <element name="RedisplayReservation" minOccurs="0">
                                        <complexType>
                                            <attribute name="waitInterval">
                                                <annotation>
                                                    <documentation>
                                                        "waitInterval" is used to specify an interval that the system should wait in milliseconds before making redisplay service calls.
                                                    </documentation>
                                                </annotation>
                                                <simpleType>
                                                    <restriction base="integer">
                                                        <minInclusive value="100"/>
                                                        <maxInclusive value="10000"/>
                                                    </restriction>
                                                </simpleType>
                                            </attribute>
                                            <attribute name="returnExtendedPriceQuote" type="boolean" default="false">
                                                <annotation>
                                                    <documentation>Used to request extended price quote display in the service response. The extended content will be passed at .../TravelItineraryRead/PriceQuote.</documentation>
                                                </annotation>
                                            </attribute>
                                        </complexType>
                                    </element>
                                </sequence>
                                <attribute name="unmaskCreditCard" type="boolean" default="false">
                                    <annotation>
                                        <documentation>Used to unmask credit card information in the TIR response. Please note that this attribute has effect only if a user has EPR keyword CCVIEW</documentation>
                                    </annotation>
                                </attribute>
                            </complexType>
                        </element>
                        <element name="SPInternal" minOccurs="0">
                            <annotation>
                                <documentation>Used for internal processing</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <any namespace="##any" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
                                </sequence>
                                <anyAttribute namespace="##any" processContents="skip"/>
                            </complexType>
                        </element>
                    </sequence>
                    <attribute name="targetCity">
                        <annotation>
                            <documentation>Used to specify to which city service should change context using ContextChange (AAA) and the rest of the service uses new security. If empty or equals current city the context doesn't change.</documentation>
                        </annotation>
                        <simpleType>
                            <restriction base="string">
                                <minLength value="3"/>
                                <maxLength value="4"/>
                            </restriction>
                        </simpleType>
                    </attribute>
                    <attribute name="haltOnInvalidMCT" type="boolean">
                        <annotation>
                            <documentation>Used to specify whether the service should check and stop processing upon encountering an error indicating that minimum connection time between air itinerary segments is not met.</documentation>
                        </annotation>
                    </attribute>
                    <attribute name="haltOnAirPriceError" type="boolean">
                        <annotation>
                            <documentation>Used to specify whether the service should stop processing upon encountering a pricing error</documentation>
                        </annotation>
                    </attribute>
                    <attribute name="haltOnHotelBookError" type="boolean">
                        <annotation>
                            <documentation>Used to specify whether the service should stop processing upon encountering a hotel book error.</documentation>
                        </annotation>
                    </attribute>
                    <attribute name="haltOnVehicleBookError" type="boolean" default="false">
                        <annotation>
                            <documentation>Used to specify whether the service should stop processing upon encountering a vehicle book error.</documentation>
                        </annotation>
                    </attribute>
                    <attribute name="healthCheck" type="boolean" default="false">
                        <annotation>
                            <documentation>Health Check.
                                For internal use only.</documentation>
                        </annotation>
                    </attribute>
                </extension>
            </complexContent>
        </complexType>
    </element>
</schema>
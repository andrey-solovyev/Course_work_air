                            Possible values:
                            IT = inclusive tour
                            BT = bulk tour


                            Ref: DM0Z_65_ID02
                            #DM0Z_65_TOUI
                            #DM0Z_65_TOUB


                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="Code.TourType">
        <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>


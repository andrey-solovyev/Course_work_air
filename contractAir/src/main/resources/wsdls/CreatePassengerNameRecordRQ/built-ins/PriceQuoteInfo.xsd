<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:element name="PriceQuoteInfo">
                    <xs:annotation>
                        <xs:documentation>Price Quote information stored in single PNR

                            Ticketing System can get more than one PNR per transaction. For the initial implementation the maximum is one PNR at a time
                            and attempt to seach for Price Quotes in more than one PNR will result in error</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Reservation" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Includes the PNR number</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="PNR_Locator">
                                            <xs:attribute name="createDate" type="xs:dateTime">
                                                <xs:annotation>
                                                    <xs:documentation>The date when the reservation has been made</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="updateToken" use="required">
                                                <xs:annotation>
                                                    <xs:documentation>Update token indicator which is required value to be passed in any update operation</xs:documentation>
                                                </xs:annotation>
                                                <xs:simpleType>
                                                    <xs:restriction base="xs:string">
                                                        <xs:minLength value="1" />
                                                        <xs:maxLength value="255" />
                                                    </xs:restriction>
                                                </xs:simpleType>
                                            </xs:attribute>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="Summary">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="NameAssociation" minOccurs="0" maxOccurs="unbounded">
                                            <xs:annotation>
                                                <xs:documentation>Defines association of a passenger to the Price Quote and the Price Quote items. This information is equivalent
                                                    to *PQS</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="PriceQuote" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="Indicators" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>PQ indicators with boolen values; default to 'false'</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="ManualPricing" minOccurs="0">
                                                                                <xs:complexType>
                                                                                    <xs:attribute name="verifyFareCalculation" type="xs:boolean">
                                                                                        <xs:annotation>
                                                                                            <xs:documentation>Ref:  DM0Z_40_PID1
                                                                                                #DM0Z_40_VRF</xs:documentation>
                                                                                        </xs:annotation>
                                                                                    </xs:attribute>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                        <xs:attribute name="segmentSelect" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicator to specify itinerary  was priced by selecting segment</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ticketed" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicator to specify PQ has been ticketed</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="override" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicator to specify PQ has been override</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="deleted" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Specifies that this is a deleted PQ</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="itineraryChange" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicator to specify that the itinerary has changed</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="Passenger" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Passenger data related to PQ</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:complexType>
                                                                        <xs:attribute name="type">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Passenger Type
                                                                                    Ref: DM0Z_40_PTCD</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:maxLength value="3" />
                                                                                    <xs:pattern value="[A-Z0-9]{3}" />
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="requestedType">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Input Passenger Type.
                                                                                    Ref: DM0Z_40_PTCI</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:maxLength value="3" />
                                                                                    <xs:pattern value="[A-Z0-9]{3}" />
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="pricedType">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Passenger Type priced.
                                                                                    Ref: DM0Z_40_TPTC</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:maxLength value="3" />
                                                                                    <xs:pattern value="[A-Z0-9]{3}" />
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="passengerTypeCount" type="xs:int">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Count of passenger type in the Price Quote</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="ItineraryType" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Possible values
                                                                            I - International
                                                                            D- Domestic

                                                                            Ref:DM0Z_65_ID01</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:simpleType>
                                                                        <xs:restriction base="xs:string">
                                                                            <xs:maxLength value="1" />
                                                                        </xs:restriction>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                                <xs:element name="Fee" minOccurs="0" maxOccurs="unbounded">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Only 50 fee items can be stored per PQ</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="Amount">
                                                                                <xs:annotation>
                                                                                    <xs:documentation>Fee Amount</xs:documentation>
                                                                                </xs:annotation>
                                                                                <xs:complexType>
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Amount</xs:documentation>
                                                                                    </xs:annotation>
                                                                                    <xs:simpleContent>
                                                                                        <xs:extension base="Amount.Basic">
                                                                                            <xs:attribute name="currencyCode">
                                                                                                <xs:simpleType>
                                                                                                    <xs:restriction base="xs:string">
                                                                                                        <xs:maxLength value="3" />
                                                                                                    </xs:restriction>
                                                                                                </xs:simpleType>
                                                                                            </xs:attribute>
                                                                                            <xs:attribute name="decimalPlace">
                                                                                                <xs:simpleType>
                                                                                                    <xs:restriction base="xs:integer">
                                                                                                        <xs:whiteSpace value="collapse" fixed="true" />
                                                                                                        <xs:fractionDigits value="0" fixed="true" />
                                                                                                        <xs:pattern value="[\-+]?[0-9]+" />
                                                                                                    </xs:restriction>
                                                                                                </xs:simpleType>
                                                                                            </xs:attribute>
                                                                                        </xs:extension>
                                                                                    </xs:simpleContent>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                            <xs:element name="Waiver" minOccurs="0">
                                                                                <xs:annotation>
                                                                                    <xs:documentation>A reason code used to override the fee for the purpose of not collecting any payment for that fee. When
                                                                                        override functionality is used, the transaction will be logged to the overrride report.</xs:documentation>
                                                                                </xs:annotation>
                                                                                <xs:simpleType>
                                                                                    <xs:restriction base="xs:string">
                                                                                        <xs:maxLength value="3" />
                                                                                    </xs:restriction>
                                                                                </xs:simpleType>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                        <xs:attribute name="type" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Group or type which the fee belongs to.

                                                                                    Possible Values:
                                                                                    OB</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:maxLength value="2" />
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="code" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Code associated to the fee.

                                                                                    Possible values:
                                                                                    R01
                                                                                    R02-R12
                                                                                    T01
                                                                                    T02-T20</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:maxLength value="3" />
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="itemId" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Fee item identifier that uniquely identifies this fee association to the name</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:integer">
                                                                                    <xs:whiteSpace value="collapse" fixed="true" />
                                                                                    <xs:fractionDigits value="0" fixed="true" />
                                                                                    <xs:minInclusive value="1" />
                                                                                    <xs:pattern value="[\-+]?[0-9]+" />
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="TicketDesignator" minOccurs="0" maxOccurs="2">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Code assigned by the airline/subscribers to designate specific fare discounts</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:simpleType>
                                                                        <xs:restriction base="xs:string">
                                                                            <xs:maxLength value="15" />
                                                                        </xs:restriction>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                                <xs:element name="ValidatingCarrier" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Validating Carrier.
                                                                            Ref: DM0Z_65_CRXC</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:simpleType>
                                                                        <xs:restriction base="xs:string">
                                                                            <xs:pattern value="[A-Z0-9]{2}[A-Z]?" />
                                                                        </xs:restriction>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                                <xs:element name="Amounts" minOccurs="0">
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="Total" minOccurs="0">
                                                                                <xs:annotation>
                                                                                    <xs:documentation>Total fare inclusive taxes per PQ type</xs:documentation>
                                                                                </xs:annotation>
                                                                                <xs:complexType>
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Amount</xs:documentation>
                                                                                    </xs:annotation>
                                                                                    <xs:simpleContent>
                                                                                        <xs:extension base="Amount.Basic">
                                                                                            <xs:attribute name="currencyCode">
                                                                                                <xs:simpleType>
                                                                                                    <xs:restriction base="xs:string">
                                                                                                        <xs:maxLength value="3" />
                                                                                                    </xs:restriction>
                                                                                                </xs:simpleType>
                                                                                            </xs:attribute>
                                                                                            <xs:attribute name="decimalPlace">
                                                                                                <xs:simpleType>
                                                                                                    <xs:restriction base="xs:integer">
                                                                                                        <xs:whiteSpace value="collapse" fixed="true" />
                                                                                                        <xs:fractionDigits value="0" fixed="true" />
                                                                                                        <xs:pattern value="[\-+]?[0-9]+" />
                                                                                                    </xs:restriction>
                                                                                                </xs:simpleType>
                                                                                            </xs:attribute>
                                                                                        </xs:extension>
                                                                                    </xs:simpleContent>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                            <xs:element name="ReissueDifference" minOccurs="0">
                                                                                <xs:annotation>
                                                                                    <xs:documentation>Reissue  difference amount (for add collect, refund or even exchange)</xs:documentation>
                                                                                </xs:annotation>
                                                                                <xs:complexType>
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Amount</xs:documentation>
                                                                                    </xs:annotation>
                                                                                    <xs:simpleContent>
                                                                                        <xs:extension base="Amount.Basic">
                                                                                            <xs:attribute name="currencyCode">
                                                                                                <xs:simpleType>
                                                                                                    <xs:restriction base="xs:string">
                                                                                                        <xs:maxLength value="3" />
                                                                                                    </xs:restriction>
                                                                                                </xs:simpleType>
                                                                                            </xs:attribute>
                                                                                            <xs:attribute name="decimalPlace">
                                                                                                <xs:simpleType>
                                                                                                    <xs:restriction base="xs:integer">
                                                                                                        <xs:whiteSpace value="collapse" fixed="true" />
                                                                                                        <xs:fractionDigits value="0" fixed="true" />
                                                                                                        <xs:pattern value="[\-+]?[0-9]+" />
                                                                                                    </xs:restriction>
                                                                                                </xs:simpleType>
                                                                                            </xs:attribute>
                                                                                        </xs:extension>
                                                                                    </xs:simpleContent>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="LocalCreateDateTime" type="xs:dateTime" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Local Create Date/time of  the PQ.
                                                                            Ref: SR06PQ-20_CLDB/CTIM</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                                <xs:element name="MessageInfo" minOccurs="0" maxOccurs="unbounded">
                                                                    <xs:complexType>
                                                                        <xs:simpleContent>
                                                                            <xs:extension base="String.Remarks">
                                                                                <xs:attribute name="type" type="xs:string">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>ERROR
                                                                                            WARNING
                                                                                            INFO

                                                                                            Ref:  DM0Z_60_TYPE

                                                                                            x'0C,04,11,03,13</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:attribute>
                                                                                <xs:attribute name="number" type="xs:integer">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Message number

                                                                                            Ref: DM0Z_60_MNBR</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:attribute>
                                                                            </xs:extension>
                                                                        </xs:simpleContent>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                            <xs:attribute name="type" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>Price Quote Type

                                                                        Possible values can be:
                                                                        PQ - Price Quote Record
                                                                        PQR - Price Quote Reissue</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:enumeration value="PQ" />
                                                                        <xs:enumeration value="PQR" />
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                            <xs:attribute name="number" type="xs:integer" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>PQ sequence number as stored and displayed from the database</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="status" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>PQ status
                                                                        Possible values are:
                                                                        A - Active
                                                                        H - History
                                                                        below values applicable only for airlines and not subscriber
                                                                        I - Inactive
                                                                        M - Manual
                                                                        X - Expired
                                                                        U - Unretained
                                                                        F - FSC Manual</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:minLength value="1" />
                                                                        <xs:maxLength value="2" />
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                            <xs:attribute name="pricingType" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>Code associated to the status as to how pricing was done

                                                                        Possible values:
                                                                        S - System
                                                                        M - Manual
                                                                        A - Amended</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:minLength value="1" />
                                                                        <xs:maxLength value="2" />
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                            <xs:attribute name="pricingStatus" type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>pricing status applicable for Abacus/Infini

                                                                        Possible values:

                                                                        Abacus GDS
                                                                        AUTO PRICED
                                                                        MANUAL
                                                                        RATE DESK

                                                                        INFINI GDS
                                                                        C-Guaranteed
                                                                        W - With warning/Not guaranteed
                                                                        M - amendments/Not guaranteed
                                                                        A- overrides/Not guaranteed</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="reissueType" type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Applicable only for a reissued PQ(PQR) and type is of "PQReissue"
                                                                        Possible values:
                                                                        INVOL
                                                                        SKED CHG
                                                                        EXCH EVEN
                                                                        EXCH CF
                                                                        EXCH AC
                                                                        EXCH AC/CF
                                                                        EXCH RFND
                                                                        LTA
                                                                        AFFIDAVIT
                                                                        LTA/CF
                                                                        AFFIDAVIT/CF
                                                                        REFUND
                                                                        REFUND CF</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="latestPQFlag" type="xs:boolean">
                                                                <xs:annotation>
                                                                    <xs:documentation>This flag indicates if this is the latest PQ applicable for the Passenger. Applicable only for Associated PQ</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="viewershipRestricted" type="xs:boolean">
                                                                <xs:annotation>
                                                                    <xs:documentation>This flag iindicates if the PQ is viewable or restricted. Set to true when viewership is restricted.</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="lowCostCarrier" type="xs:boolean">
                                                                <xs:annotation>
                                                                    <xs:documentation>PQ created to be ticketed by EasyJet, Instant Purchase Carrier (IPC), Low Cost Carrier (LCC) or Hybrid Carrier (HCPQ)
                                                                        Ref: #DM0Z_40_HCPQ
                                                                        Value is false if attribute not present.</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                                <xs:attribute name="nameNumber">
                                                    <xs:annotation>
                                                        <xs:documentation>A sequence number of the customer name as it is stored in the reservation record.</xs:documentation>
                                                    </xs:annotation>
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:string">
                                                            <xs:minLength value="1" />
                                                            <xs:maxLength value="5" />
                                                            <xs:pattern value="[0-9]{1,2}\.[0-9]{1,2}" />
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                </xs:attribute>
                                                <xs:attribute name="nameId" type="xs:integer" use="required">
                                                    <xs:annotation>
                                                        <xs:documentation>A unique number associated to the customer/traveler name in the reservation record (PNR)</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="firstName">
                                                    <xs:annotation>
                                                        <xs:documentation>First Name Ref: SR06PQ_30_FST</xs:documentation>
                                                    </xs:annotation>
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:string">
                                                            <xs:minLength value="1" />
                                                            <xs:maxLength value="63" />
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                </xs:attribute>
                                                <xs:attribute name="lastName">
                                                    <xs:annotation>
                                                        <xs:documentation>Last Name Ref: SR06PQ_30_LST</xs:documentation>
                                                    </xs:annotation>
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:string">
                                                            <xs:minLength value="1" />
                                                            <xs:maxLength value="63" />
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="NoNameAssociation" minOccurs="0" maxOccurs="unbounded">
                                            <xs:annotation>
                                                <xs:documentation>Defines Price Quotes that are not name associated in the summary (part of  equivalent *PQS)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="PriceQuote" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="Indicators" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>PQ indicators with boolen values; default to 'false'</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="ManualPricing" minOccurs="0">
                                                                                <xs:complexType>
                                                                                    <xs:attribute name="verifyFareCalculation" type="xs:boolean">
                                                                                        <xs:annotation>
                                                                                            <xs:documentation>Ref:  DM0Z_40_PID1
                                                                                                #DM0Z_40_VRF</xs:documentation>
                                                                                        </xs:annotation>
                                                                                    </xs:attribute>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                        <xs:attribute name="segmentSelect" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicator to specify itinerary  was priced by selecting segment</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ticketed" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicator to specify PQ has been ticketed</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="override" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicator to specify PQ has been override</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="deleted" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Specifies that this is a deleted PQ</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="itineraryChange" type="xs:boolean">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicator to specify that the itinerary has changed</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="Passenger" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Passenger data related to PQ</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:complexType>
                                                                        <xs:attribute name="type">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Passenger Type
                                                                                    Ref: DM0Z_40_PTCD</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
